{
  "name": "juna-supabase",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imuradevelopment/Juna_supabase.git"
  },
  "homepage": "https://juna-supabase.netlify.app",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "analyze": "vite build --mode analyze",
    "deps": "depcruise --config .dependency-cruiser.cjs --include-only \"^src\" --output-type dot src | dot -T svg > dependency-graph.svg",
    "db:reset": "yes | supabase db reset --linked",
    "db:push": "supabase db push --include-all",
    "functions:deploy": "npm run functions:deploy:register && npm run functions:deploy:login && npm run functions:deploy:delete",
    "functions:deploy:register": "supabase functions deploy register-user --no-verify-jwt",
    "functions:deploy:login": "supabase functions deploy login-with-account --no-verify-jwt",
    "functions:deploy:delete": "supabase functions deploy delete-user --no-verify-jwt",
    "supabase:link": "supabase link --project-ref qwsbdmkvozkfpznfzjay",
    "reset:all": "npm run db:reset && npm run db:push && npm run functions:deploy"
  },
  "dependencies": {
    "@phosphor-icons/vue": "^2.1.6",
    "@supabase/supabase-js": "^2.39.7",
    "@tiptap/extension-color": "^2.2.4",
    "@tiptap/extension-image": "^2.2.4",
    "@tiptap/extension-link": "^2.2.4",
    "@tiptap/extension-placeholder": "^2.2.4",
    "@tiptap/extension-text-align": "^2.2.4",
    "@tiptap/extension-text-style": "^2.2.4",
    "@tiptap/extension-underline": "^2.2.4",
    "@tiptap/pm": "^2.2.4",
    "@tiptap/starter-kit": "^2.2.4",
    "@tiptap/vue-3": "^2.2.4",
    "browser-image-compression": "^2.0.2",
    "date-fns": "^3.3.1",
    "dompurify": "^3.2.4",
    "lodash": "^4.17.21",
    "marked": "^15.0.7",
    "pinia": "^2.1.7",
    "uuid": "^11.1.0",
    "vue": "^3.4.38",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@types/dompurify": "^3.0.5",
    "@types/lodash": "^4.17.16",
    "@types/marked": "^5.0.2",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-vue": "^5.1.3",
    "autoprefixer": "^10.4.18",
    "dependency-cruiser": "^16.10.0",
    "postcss": "^8.4.35",
    "postcss-custom-properties": "^14.0.4",
    "postcss-import": "^16.1.0",
    "postcss-preset-env": "^10.1.5",
    "rollup-plugin-visualizer": "^5.14.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "vite": "^6.2.2",
    "vue-tsc": "^2.1.4"
  }
}
